datasource db {
  url      = env("DATABASE_URL")
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model Song {
  id        Int    @id @default(autoincrement())
  videoId   String
  title     String
  thumbnail String
  duration  Int
  started Boolean @default(false)
  ended   Boolean @default(false)
  skipped   Boolean @default(false)
  createdAt DateTime @default(now())
  requester  String
}
