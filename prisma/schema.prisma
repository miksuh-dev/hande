datasource db {
  url      = env("DATABASE_URL")
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model Song {
  id        Int    @id @default(autoincrement())
  videoId   String
  title     String
  serverHash String
  thumbnail String
  startedAt  DateTime?
  endedAt   DateTime?
  createdAt DateTime @default(now())
  requester  String
}
